{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-starter/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-starter/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "nest-starter": {
      "type": "application",
      "root": "apps/nest-starter",
      "entryFile": "main",
      "sourceRoot": "apps/nest-starter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-starter/tsconfig.app.json"
      }
    },
    "second-project": {
      "type": "application",
      "root": "apps/second-project",
      "entryFile": "main",
      "sourceRoot": "apps/second-project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/second-project/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/nest-starter"
}